# fit parameters DON'T CHANGE!!!!!!!!
lm_0	1
lm_up	5
lm_down	10
#################################

# verbosity on (1) or off (0)
verbose	1
fast	0

#true oscillation parametres to be fitted
#name	"asim"
#M12	7.53e-5
#M23	2.509e-3
#S12	0.304
#S13	0.0219
#S23	0.528
#dCP	-1.5708
Point	49773

#asim space
parm_M12	0.0000753	0.0000753	1	
parm_S12	0.304		0.304		1
#fitted 
parm_S13	0.0178175	0.0256584	13
parm_M23	0.002464	0.002554	13
parm_S23	0.426		0.579		19
parm_CP		-3.14159265359	3.14159265359	61

# $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$

# *input* is a list of files with oscillation information (such as from global)
# they must contain a mcTree object
# *output* is the output path with file and extension
#input	"/data/tboschi/HKsens/OscAna/SuperHK/.tmp_moms"
output	"/data/tboschi/HKsens/errorstudy/0/asim/NH_NH/sensitivity/moms/SpaghettiSens.root"
# ____________________________________________________

# need a version of CardDelaer which can handle string arrays
sample	"E_FHC"	"E_RHC"	"M_FHC"	"M_RHC"
#energy scale error for E or M
#scale	0.024	if just one
scale_E	 0.024
scale_M  0.024

#sample	"M_FHC"	"M_RHC"
#sample	"atmo"

# oscillation stuff
#
#earth density profile between JPARK and SK
density_profile	"data/DensityProfileTochibora.dat"
true_hierarchy	"normal"
fit_hierarchy	"normal"

#proportion of statistics
stats	1.00

#restrict systematics
#flux  from [0, 50)
#xsec  from [50, 74)
#skdet from [74, 119)
#syst_first	0
#syst_last	50

#systematic informations
corr_file	"/data/tboschi/HKsens/errorstudy/0/systematics/combinedmatrix.root"
corr_name	"correlation"

systematic_E_FHC "/data/tboschi/HKsens/errorstudy/0/systematics/FHC1Re.fij.t2k_spline.root"
systematic_E_RHC "/data/tboschi/HKsens/errorstudy/0/systematics/RHC1Re.fij.t2k_spline.root"
systematic_M_FHC "/data/tboschi/HKsens/errorstudy/0/systematics/FHC1Rmu.fij.t2k_spline.root"
systematic_M_RHC "/data/tboschi/HKsens/errorstudy/0/systematics/RHC1Rmu.fij.t2k_spline.root"




